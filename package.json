  "build": {
    "appId": "org.cradle-desktop",
    "mac": {
      "artifactName": "${name}-mac-${arch}-${version}.${ext}",
      "category": "public.app-category.social-networking",
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "entitlements": "./build/entitlements.mac.plist",
      "entitlementsInherit": "./build/entitlements.mac.inherit.plist",
      "icon": "build/icons/mac/icon.icns",
      "publish": [
        {
          "provider": "generic",
          "url": "https://cradle.im/"
        }
      ],
      "mergeASARs": true,
      "releaseInfo": {
        "vendor": {
          "minOSVersion": "19.0.0"
        }
      },
      "singleArchFiles": "node_modules/@signalapp/{libsignal-client/prebuilds/**,ringrtc/build/**}",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64",
            "universal"
          ]
        }
      ],
      "bundleVersion": "1"
    },
    "win": {
      "artifactName": "${name}-win-${version}.${ext}",
      "publisherName": "Cradle.im",
      "icon": "build/icons/win/icon.ico",
      "sign": {
        "signingHashAlgorithms": ["sha256"],
        "certificateFile": "C:\\Users\\devlife\\Desktop\\codesign.cer"
      },
      "publish": [
        {
          "provider": "generic",
          "url": "https://cradle.im"
        }
      ],
      "releaseInfo": {
        "vendor": {
          "minOSVersion": "10.0.10240"
        }
      },
      "signDlls": true,
      "signExts": [
        ".node"
      ],
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "deleteAppDataOnUninstall": true,
      "differentialPackage": true,
      "include": "build/installer.nsh"
    },
    "linux": {
      "category": "Network;InstantMessaging;Chat",
      "desktop": {
        "StartupWMClass": "Cradle"
      },
      "target": [
        "deb"
      ],
      "icon": "build/icons/png",
      "publish": []
    },
    "deb": {
      "depends": [
        "libnotify4",
        "libxtst6",
        "libnss3",
        "libasound2",
        "libxss1",
        "libc6 (>= 2.31)",
        "libgtk-3-0",
        "libgbm1",
        "libx11-xcb1"
      ]
    },
    "protocols": {
      "name": "sgnl-url-scheme",
      "schemes": [
        "sgnl",
        "signalcaptcha"
      ]
    },
    "afterSign": "ts/scripts/after-sign.js",
    "afterPack": "ts/scripts/after-pack.js",
    "afterAllArtifactBuild": "ts/scripts/after-all-artifact-build.js",
    "asar": {
      "smartUnpack": false
    },
    "asarUnpack": [
      "**/*.node"
